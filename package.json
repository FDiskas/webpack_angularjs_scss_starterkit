{
  "name": "webpack2_test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:dev": "rimraf dist && webpack --env=dev --progress --colors --define process.env.NODE_ENV=development",
    "build:prod": "rimraf dist && webpack --env=prod --progress --colors  --progress --optimize-minimize --define process.env.NODE_ENV=production",
    "server:dev": "webpack-dev-server --env=dev --progress --colors --watch --cache --open --hot --inline --define process.env.NODE_ENV=development",
    "server:prod": "pushstate-server ./dist",
    "start": "npm run server:dev",
    "build": "npm run build:prod"
  },
  "devDependencies": {
    "@types/node": "^7.0.22",
    "angular-template-url-loader": "^1.0.2",
    "awesome-typescript-loader": "^3.1.3",
    "css-loader": "0.12.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-minifier": "^3.5.2",
    "html-webpack-plugin": "^2.28.0",
    "ngxtemplate-loader": "^1.0.3",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "pushstate-server": "^3.0.0",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "sassc-loader": "https://github.com/FDiskas/sassc-loader.git#patch-1",
    "style-loader": "^0.18.1",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.3.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@types/angular": "1.6.17",
    "@types/jquery": "^2.0.45",
    "angular": "1.8.0",
    "bootstrap-sass": "^3.4.1",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.0"
  }
}
